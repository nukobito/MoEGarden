{
    "manifest_version": 2,
    "name": "もえ栽培",
    "version": "0.1",
    "description": "Master of Epic栽培用タイマー",
    "icons": {
        "128": "icons/icon128x128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "icons/icon19x19.png"
        },
        "default_title": "もえ栽培",
        "default_popup": "popup.html"
    },
    "background": {
        "persistent": true,
        "page": "background.html"
    },
    "permissions": [
        "alarms",
        "storage"
    ]
}